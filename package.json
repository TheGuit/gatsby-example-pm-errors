{
  "workspaces": [
    "site",
    "themes/*"
  ],
  "private": true,
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prettier": "prettier \"**/*.{json,md,scss,yaml,yml}\"",
    "test-not-js": "prettier --check \"**/*.{json,md,scss,yaml,yml}\"",
    "test-js": "eslint --max-warnings 0 --ext \".js,.jsx\" ."
  },
  "packageManager": "yarn@2.4.2"
}
